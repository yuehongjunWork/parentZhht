<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- dao接口层与mybatis关联 -->
<mapper namespace="cn.com.core.dao.MoneyTbDao">
	<!--  添加测试数据  -->
	<!-- 在mybatis-config.xml配置了别名路径，否则parameterType="cn.itcast.core.bean.TestTb" -->
	<insert id="insertMoneyTb" parameterType="MoneyTb">
		insert into Money_tb
		(name,password)
		values
		(#{name},#{password})
	</insert>
	<insert id="insertMoneyTb2" parameterType="MoneyTb">
		insert into Money_tb
		(name,password)
		values
		(#{name},#{password})
	</insert>
</mapper>
